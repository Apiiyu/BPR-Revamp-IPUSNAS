{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "@vue/eslint-config-prettier",
    "./.eslintrc-auto-import.json"
  ],
  "rules": {
    "vue/component-tags-order": [
      "error",
      {
        "order": ["template", "script", "style"]
      }
    ]
  }
}
